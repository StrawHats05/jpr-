import java.util.Scanner;

 public class q8 {
 int a, b;

 q8() {
 a = 0;
 b = 0;
 }

 q8(int x, int y) {
 a = x;
 b = y;
 }

 void display() {
 System.out.println("a = " + a + " b = " + b);
 }

 public static void main(String[] args) {
 Scanner sc = new Scanner(System.in);
 System.out.println("Enter two numbers: ");
 int x = sc.nextInt();
 int y = sc.nextInt();
 q8 obj1 = new q8();
 q8 obj2 = new q8(x, y);
 obj1.display();
 obj2.display();
 }
 }
