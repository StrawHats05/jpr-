
 import java.util.Scanner;

 public class q5 {
 public static void main(String[] args) {
 Scanner sc = new Scanner(System.in);
 int sum = 0;
 System.out.println("Even numbers between 1 to 10 are: ");
 for (int i = 1; i <= 10; i++) {
 if (i % 2 == 0) {
 System.out.print(i + " ");
 sum += i;
 }
 }
 System.out.println("\nSum of even numbers between 1 to 10
is: " + sum);
 }
 }
